  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist",
    "target": "es2016",
    "module": "commonjs",
    "strict": true,
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules"]
}
